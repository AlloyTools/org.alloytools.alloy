{
  "name": "org-alloytools-alloy",
  "bin": {"alloy": "jdeploy-bundle/jdeploy.js"},
  "author": "alloytools@aQute.biz",
  "description": "Alloy, a formal specification tool",
  "main": "index.js",
  "preferGlobal": true,
  "repository": "https://github.com/Alloytools/org.alloytools.alloy",
  "version": "6.1.0",
  "jdeploy": {
    "jdk": false,
    "javaVersion": "17",
    "jar": "org.alloytools.alloy.dist/target/org.alloytools.alloy.dist.jar",
    "javafx": false,
    "bundles": ["mac-x64", "mac-arm64", "win", "linux"],
    "installers" : ["mac-x64", "mac-arm64", "win", "linux"],
    "documentTypes" : [
      {
        "extension" : "als",
        "mimetype" : "text/alloy",
        "editor" : true
      }, {
        "extension" : "md",
        "mimetype" : "text/markdown",
        "editor" : true
      }
    ],
    "urlSchemes": ["alloy"],
    "title": "Alloy"
  },
  "dependencies": {
    "command-exists-promise": "^2.0.2",
    "node-fetch": "2.6.7",
    "tar": "^4.4.8",
    "yauzl": "^2.10.0",
    "shelljs": "^0.8.4"
  },
  "license": "ASL 2.0",
  "files": ["jdeploy-bundle"],
  "scripts": {"test": "echo \"Error: no test specified\" && exit 1"}
}